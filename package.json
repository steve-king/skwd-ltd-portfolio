{
  "author": "Stephen King",
  "name": "skwd-ltd-portfolio",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./public/* && rm -rf ./logs/*",
    "testDev": "NODE_PATH=./ NODE_ENV=test mocha ./tests --recursive --watch",
    "test": "NODE_PATH=./ NODE_ENV=test mocha ./tests --recursive",
    "serverDev": "NODE_ENV=development NODE_PATH=./ nodemon app/index.js >> logs/server.log",
    "serverBuild": "NODE_PATH=./ node ./app/index.js",
    "webpackDev": "webpack --watch >> logs/webpack.log",
    "browsersyncDev": "browser-sync start --proxy --open false \"localhost:5000\" --files \"app/**/*\"",
    "start": "npm test && npm run clean && npm run webpackDev | npm run serverDev",
    "build": "npm test && npm run clean && webpack -p && npm run serverBuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steve-king/skwd-ltd-portfolio.git"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "js-yaml": "^3.8.4",
    "jsonwebtoken": "^7.4.1",
    "morgan": "^1.8.2",
    "mysql2": "^1.3.5",
    "sequelize": "^4.1.0",
    "webpack": "^2.6.1"
  },
  "devDependencies": {
    "browser-sync": "^2.18.12",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "dotenv": "^4.0.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "sinon": "^2.3.4"
  }
}
