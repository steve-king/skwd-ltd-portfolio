{
  "author": "Stephen King",
  "name": "skwd-ltd-portfolio",
  "version": "1.3.13",
  "scripts": {
    "clean": "rm -rf ./_www/* && rm -rf ./logs/*",
    "testDev": "NODE_PATH=./ NODE_ENV=test mocha ./tests --recursive --watch",
    "test": "NODE_PATH=./ NODE_ENV=test mocha ./tests --recursive",
    "devServer": "NODE_ENV=development NODE_PATH=./ nodemon app/index.js > logs/server.log",
    "webpackDev": "webpack-dev-server --config webpack-dev-server.config.js",
    "start": "npm run clean && npm run devServer | webpack -w",
    "build": "npm run clean && webpack -p && npm run serve",
    "heroku-postbuild": "webpack -p --progress",
    "serve": "NODE_PATH=./ node ./app/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steve-king/skwd-ltd-portfolio.git"
  },
  "engines": {
    "node": "8.1.4"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-syntax-object-rest-spread": "^6.19.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "body-parser": "^1.17.2",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "eslint": "^4.1.0",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "imagemin-webpack-plugin": "^1.4.4",
    "is-empty-object": "^1.1.1",
    "js-yaml": "^3.8.4",
    "jsonwebtoken": "^7.4.1",
    "mime": "^1.3.6",
    "mocha": "^3.4.2",
    "morgan": "^1.8.2",
    "mysql2": "^1.3.5",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "normalize.css": "^7.0.0",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
