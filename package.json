    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-page-transition": "^3.0.2",
    "react-transition-group": "^1.2.0",
    "redux": "^3.7.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.2.0",
    "sinon": "^2.3.5",
    "svg-fill-loader": "0.0.8",
    "svg-url-loader": "^2.0.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.5.0"
  }
}
