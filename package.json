{
  "author": "Stephen King",
  "name": "skwd-ltd-portfolio",
  "version": "1.3.3",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./_www/* && rm -rf ./logs/*",
    "testDev": "NODE_PATH=./ NODE_ENV=test mocha ./tests --recursive --watch",
    "test": "NODE_PATH=./ NODE_ENV=test mocha ./tests --recursive",
    "serverDev": "NODE_ENV=development NODE_PATH=./ nodemon app/index.js > logs/server.log",
    "serverBuild": "NODE_PATH=./ node ./app/index.js",
    "webpackDev": "webpack --watch > logs/webpack.log",
    "browsersync": "browser-sync start --proxy --open false \"localhost:5000\" --files \"app/**/*\"",
    "start": "npm run clean && npm run webpackDev | npm run serverDev",
    "build": "npm run clean && webpack -p && npm run serverBuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steve-king/skwd-ltd-portfolio.git"
  },
  "engines": {
    "node": "8.1.1",
    "npm": "3.9.6"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "js-yaml": "^3.8.4",
    "jsonwebtoken": "^7.4.1",
    "morgan": "^1.8.2",
    "mysql2": "^1.3.5",
    "normalize.css": "^7.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "browser-sync": "^2.18.12",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "imagemin-webpack-plugin": "^1.4.4",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.4",
    "svg-fill-loader": "0.0.8",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1"
  }
}
